<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/relativeLayout_KuaiSuPaiXu">
    <TextView
        android:layout_width="match_parent"
        android:id="@+id/textView_TitleName"
        android:layout_height="30dp"
        android:textAlignment="center"
        android:gravity="center_vertical|left"
        ></TextView>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:id="@+id/textView_DisplayNum"
        android:gravity="center_vertical|left"
        android:textSize="25dp"
        android:layout_marginTop="30dp"
        android:textAlignment="center"
        android:background="@color/c_blue"
        ></TextView>
    <TextView
        android:layout_width="70dp"
        android:layout_height="30dp"
        android:id="@+id/textView_UnSort"
        android:text="重归混沌"
        android:textAlignment="center"
        android:gravity="center_vertical|left"
        android:layout_marginTop="90dp"
        android:layout_marginLeft="20dp"
        android:background="@color/shudu_dark"
        ></TextView>
    <TextView
        android:layout_width="70dp"
        android:layout_height="30dp"
        android:id="@+id/textView_Sort"
        android:text="秩序降临"
        android:textAlignment="center"
        android:gravity="center_vertical|left"
        android:layout_marginTop="110dp"
        android:layout_marginLeft="150dp"
        android:background="@color/shudu_dark"
        ></TextView>
    <TextView
        android:layout_width="70dp"
        android:layout_height="30dp"
        android:id="@+id/textView_Principle"
        android:text="奥义所在"
        android:textAlignment="center"
        android:gravity="center_vertical|left"
        android:layout_marginTop="90dp"
        android:layout_marginLeft="240dp"
        android:background="@color/shudu_dark"
        ></TextView>
    <TextView
        android:layout_width="70dp"
        android:layout_height="30dp"
        android:id="@+id/textView_KeyCode"
        android:text="入门之道"
        android:textAlignment="center"
        android:gravity="center_vertical|left"
        android:layout_marginTop="90dp"
        android:layout_marginLeft="240dp"
        android:background="@color/shudu_dark"
        ></TextView>
    <ImageView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="130dp"
        android:visibility="invisible"
        android:id="@+id/imageView_KeyCode"
        />
    <WebView
        android:id="@+id/principleGif"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="90dp"
        android:visibility="invisible"
        android:layout_centerVertical="true" />
    <TextView
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:id="@+id/textView_PrincipleText"
        android:textAlignment="center"
        android:text="无字天书"
        android:gravity="center_vertical|left"
        android:visibility="invisible"
        android:layout_marginTop="450dp"
        android:background="@color/shudu_light"
        ></TextView>


</RelativeLayout>
